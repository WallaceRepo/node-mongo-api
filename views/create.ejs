<html lang="en">
<%- include("./partials/header.ejs") %>
<body>
  <div class="create-blog content">
    <form action="/services" method="POST"> 
      <label for="title">New Service Name:</label>
      <input type="text" id="title" name="serviceName" required>
      <label for="snippet">Service Duration:</label>
      <input type="text" id="snippet" name="serviceDuration" required>
      <label for="body">Service Materials:</label>
      <textarea id="body" name="serviceMaterials" required></textarea>
      <label for="body">Service Price:</label>
      <textarea id="body" name="servicePrice" required></textarea>
      <button>Submit</button>
    </form>
  </div>

  <%- include("./partials/footer.ejs") %>
</body>
</html>